<dict>
	<key>FormatVersion</key>
	<string>1</string>
	<key>Rules</key>
	<array>
		<dict>
			<key>Available</key>
			<dict>
				<key>FromVersion</key>
				<string>10.14</string>
				<key>ToVersion</key>
				<string>*</string>
			</dict>
			<key>CheckAction</key>
			<dict>
				<key>Action</key>
				<string>currentUser=$( ls -l /dev/console | cut -d " " -f4 )
defaults read /Users/"$currentUser"/Library/Containers/com.apple.Safari/Data/Library/Preferences/com.apple.Safari AutoFillPasswords</string>
				<key>ResultDefault</key>
				<string></string>
				<key>Type</key>
				<string>shell</string>
			</dict>
			<key>CheckDescription</key>
			<string>With AutoFill, Safari can automatically fill in your credit card information, contact information, passwords, and more. It can automatically fill in information from any contact in Contacts. It can also generate a secure password when you need to create one. Safari highlights autofilled fields in yellow.</string>
			<key>CheckID</key>
			<string>no_autofill_username_password_safari_10_14</string>
			<key>Domain</key>
			<string>Professional</string>
			<key>Enabled</key>
			<true/>
			<key>Group</key>
			<string>safari</string>
			<key>InfoLink</key>
			<string>https://simpleum.com/en/faq-items/not-use-auto-fill-safari/</string>
			<key>InternalDocLink</key>
			<string></string>
			<key>Kind</key>
			<string>Security</string>
			<key>Meta</key>
			<dict>
				<key>Author</key>
				<string>Simpleum Media GmbH</string>
